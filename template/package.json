{
  "name": "{{#scoped}}@{{owner}}/{{/scoped}}{{name}}",
  "repository": "github:{{owner}}/{{name}}",
  "description": "{{description}}",
  "version": "{{version}}",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    {{#with-benchmark}}
    "benchmark": "tsex benchmark",
    "benchmark:watch": "tsex benchmark --watch",
    {{/with-benchmark}}
    {{#if with-tests}}
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "test": "tsex test",
    "test:watch": "tsex --watch",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
    {{else}}
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "prepublishOnly": "npm run clean && npm run compile"
    {{/if}}
  },
  "keywords": [
    "//TODO"
  ],
  "dependencies": {},
  "devDependencies": {
    {{#with-benchmark}}
    "benchloop": "^1.3.2",
    {{/with-benchmark}}
    {{#with-tests}}
    "fava": "^0.0.5",
    {{/with-tests}}
    "tsex": "^1.0.0",
    "typescript": "^4.6.3"
  }
}
